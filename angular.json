{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"vuexy": {
			"root": "",
			"sourceRoot": "src",
			"projectType": "application",
			"prefix": "app",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"allowedCommonJsDependencies": [
							"hammerjs",
							"node-waves",
							"app/main/ui/icons/feather/feather.component",
							"@angularclass/hmr",
							"ngx-quill",
							"lodash",
							"uuid",
							"nouislider",
							"dragula",
							"apexcharts",
							"chart.js",
							"contra/emitter",
							"crossvent",
							"highlight.js",
							"ng2-flatpickr"
						],
						"outputPath": "dist/vuexy",
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "src/tsconfig.app.json",
						"assets": [
							"src/favicon.ico",
							"src/assets"
						],
						"styles": [
							"./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
							"src/styles.scss",
							"node_modules/highlight.js/scss/atom-one-dark.scss",
							"src/assets/fonts/feather/iconfont.css",
							"src/assets/fonts/font-awesome/css/font-awesome.min.css",
							"src/assets/fonts/flag-icon-css/css/flag-icon.min.css"
						],
						"stylePreprocessorOptions": {
							"includePaths": [
								"node_modules",
								"src/assets/"
							]
						},
						"scripts": [
							"./node_modules/katex/dist/katex.min.js",
							"./node_modules/quill/dist/quill.min.js"
						],
						"vendorChunk": true,
						"extractLicenses": false,
						"buildOptimizer": false,
						"sourceMap": true,
						"optimization": false,
						"namedChunks": true,
						"extraWebpackConfig": "webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							],
							"baseHref": "/",
							"aot": true,
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "4mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4mb"
								}
							],
							"extraWebpackConfig": "webpack.prod.config.js"
						},
						"hmr": {
							"budgets": [
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4mb"
								}
							],
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.hmr.ts"
								}
							]
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"options": {
						"browserTarget": "vuexy:build",
						"port": 4200,
						"publicHost": "http://localhost:4200",
						"extraWebpackConfig": "webpack.config.js"
					},
					"configurations": {
						"production": {
							"browserTarget": "vuexy:build:production",
							"extraWebpackConfig": "webpack.prod.config.js"
						},
						"hmr": {
							"hmr": true,
							"browserTarget": "vuexy:build:hmr"
						}
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "vuexy:build",
						"extraWebpackConfig": "webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/test.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "src/tsconfig.spec.json",
						"karmaConfig": "src/karma.conf.js",
						"styles": [
							"./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
							"src/styles.scss"
						],
						"scripts": [
							"node_modules/apexcharts/dist/apexcharts.min.js"
						],
						"assets": [
							"src/favicon.ico",
							"src/assets"
						]
					}
				}
			}
		},

		"app1": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				},
				"@schematics/angular:application": {
					"strict": true
				}
			},
			"root": "projects/app1",
			"sourceRoot": "projects/app1/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/app1",
						"index": "projects/app1/src/index.html",
						"main": "projects/app1/src/main.ts",
						"polyfills": "projects/app1/src/polyfills.ts",
						"tsConfig": "projects/app1/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/app1/src/favicon.ico",
							"projects/app1/src/assets"
						],
						"styles": [
							"projects/app1/src/styles.scss"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/app1/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "4mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4mb",
									"maximumError": "5mb"
								}
							],
							"fileReplacements": [
								{
									"replace": "projects/app1/src/environments/environment.ts",
									"with": "projects/app1/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/app1/webpack.prod.config.js",
							"baseHref": "/",
							"aot": true,
							"optimization": true,
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "app1:build:production",
							"extraWebpackConfig": "projects/app1/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "app1:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4201,
						"publicHost": "http://localhost:4201",
						"extraWebpackConfig": "projects/app1/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "app1:build",
						"extraWebpackConfig": "projects/app1/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/app1/src/test.ts",
						"polyfills": "projects/app1/src/polyfills.ts",
						"tsConfig": "projects/app1/tsconfig.spec.json",
						"karmaConfig": "projects/app1/karma.conf.js",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/app1/src/favicon.ico",
							"projects/app1/src/assets"
						],
						"styles": [
							"projects/app1/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		}
	},
	"defaultProject": "vuexy",
	"cli": {
		"analytics": false,
		"packageManager": "yarn"
	}
}